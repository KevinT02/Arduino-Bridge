void setup() {
 // put your setup code here, to run once:
 
 Serial.begin(9600);
 Serial.println("Testing QTI Sensor");
 delay(1);

}

void loop() {

  pinMode(2, OUTPUT);
  digitalWrite(2, HIGH);
  delay(1);

  pinMode(2, INPUT);
  delay(1);

  byte sensor1 = digitalRead(2);
  Serial.println(sensor1);
  delay(50);


}
